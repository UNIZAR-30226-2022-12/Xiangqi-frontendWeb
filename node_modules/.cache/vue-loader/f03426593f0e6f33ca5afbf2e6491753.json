go()\" label=\"Iniciar sesión / crear cuenta\" icon=\"pi pi-check\" iconPos=\"right\" />\n  <Dialog v-model:visible=\"display\" :header=\"texto\" :draggable=\"false\" :modal=\"true\" :class=\"{ 'altura': isActive, 'alturaMax': isActiveMax }\"> <!--v-model:visible asociado con variable contentStyle=\"padding: 0px;\"-->\n    <template #header :class=\"colorHeader\">\n      <div class=\"card\">    \n        <Button class=\"p-button-outlined mr-3\" v-on:click=\"selectedTab('LogIn'), resize('LogIn')\" label=\"Iniciar sesión\" />\n        <Button class=\"p-button-outlined\" v-on:click=\"selectedTab('SignUp'), resize('SignUp')\" label=\"Crear una cuenta\" />\n      </div>\n    </template>\n    <div id=\"LogIn\" class=\"w3-container tabTypeJS\">\n      <div class=\"content-section implementation\">\n        <div class=\"card\">\n          <div class=\"p-fluid grid\" style=\"display: block !important;\">   \n            <!--FIELD-->\n            <div class=\"col-12\" style=\"padding-bottom: 0rem;\">\n              <h3> {{texto}}: </h3>\n            </div>\n            <!--USERNAME-->\n            <div class=\"field col-12\">\n              <label for=\"username\">Nombre de usuario</label>\n              <InputText id=\"username\" type=\"text\" v-model=\"username\" :class=\"{ 'p-invalid': errUserLogin}\"/>\n              <small v-if=\"errUserLogin === true\" id=\"username-help\" class=\"p-error\">Por favor, proporcione un nombre de usuario.</small>\n            </div>\n            <!--PASSWORD-->\n            <div class=\"field col-12\">\n              <label for=\"password\">Contraseña</label>\n              <Password id=\"password\" v-model=\"password\" :class=\"{ 'p-invalid': errPassLogin}\" toggleMask></Password>\n              <small v-if=\"errPassLogin === true\" id=\"password-help\" class=\"p-error\">{{textoPassLog}}</small>\n            </div>\n          </div>\n        </div>\n      </div>  \n    </div>\n\n    <div id=\"SignUp\" class=\"w3-container tabTypeJS\" style=\"display:none\">\n      <div class=\"content-section implementation\">\n        <div class=\"card\">\n            <div class=\"p-fluid grid\" style=\"display: block !important;\">\n              <!--FIELD-->\n              <div class=\"col-12\">\n                <h3> {{texto}}: </h3>\n              </div>                          \n              <!--USERNAME-->\n              <div class=\"field col-12\">\n                <label for=\"usernameSign\">Nombre de usuario:</label>\n                <InputText id=\"usernameSign\" type=\"text\" v-model=\"usernameSign\" :class=\"{ 'p-invalid': errUserSign }\"/>\n                <small v-if=\"errUserSign === true\" id=\"username-help\" class=\"p-error\">Por favor, proporcione un nombre de usuario.</small>\n              </div>\n              <!--MAIL-->\n              <div class=\"field col-12\">\n                <label for=\"mail\">E-mail:</label>\n                <InputText id=\"mail\" type=\"email\" v-model=\"mail\" :class=\"{ 'p-invalid': errMailSign }\"/>\n                <small v-if=\"errMailSign === true\" id=\"mail-help\" class=\"p-error\">Por favor, proporcione una dirección de correo válida.</small>\n              </div>\n              <!--NAME-->\n              <div class=\"field col-12\">\n                <label for=\"name\">Nombre y apellido:</label>\n                <InputText id=\"name\" type=\"text\" v-model=\"name\" :class=\"{ 'p-invalid': errNameSign }\"/>\n                <small v-if=\"errNameSign === true\" id=\"name-help\" class=\"p-error\">Por favor, proporcione su nombre.</small>\n              </div>\n              <!--DATE OF BIRTH-->\n              <div class=\"field col-12\">\n                <label for=\"calendar\">Fecha de nacimiento:</label>\n                <Calendar id=\"calendar\" v-model=\"birthday\" :class=\"{ 'p-invalid': errBdaySign }\"/>\n                <small v-if=\"errBdaySign === true\" id=\"calendar-help\" class=\"p-error\">Por favor, proporcione una fecha de nacimiento.</small>   \n              </div>\n              <!--COUNTRY-->\n              <div class=\"field col-12\">\n                <label for=\"country\">Seleccione su país de residencia:</label>\n                <Dropdown v-model=\"selectedCountry\" :options=\"countries\" optionLabel=\"name\" :filter=\"true\" placeholder=\"Seleccione su país\" :showClear=\"true\" :class=\"{ 'p-invalid': errCountrySign }\">\n                  <template #value=\"slotProps\">\n                      <div class=\"country-item country-item-value\" v-if=\"slotProps.value\">\n                          <img src=\"images/flag_placeholder.png\" :class=\"'flag flag-' + slotProps.value.code.toLowerCase()\" />\n                          {{slotProps.value.name}}\n                      </div>\n                      <span v-else>\n                          {{slotProps.placeholder}}\n                      </span>\n                  </template>\n                  <template #option=\"slotProps\">\n                      <div class=\"country-item\">\n                          <img src=\"images/flag_placeholder.png\" :class=\"'flag flag-' + slotProps.option.code.toLowerCase()\" />\n                          {{slotProps.option.name}}\n                      </div>\n                  </template>\n                </Dropdown>\n                <small v-if=\"errCountrySign === true\" id=\"country-help\" class=\"p-error\">Por favor, proporcione un nombre de usuario.</small>\n              </div>\n              <!--FOTO DE PERFIL-->\n              <div class=\"field col-12\">\n                <label for=\"profilePic\">Foto de perfil:</label>\n                <FileUpload mode=\"basic\" v-model=\"photo\" chooseLabel=\"Seleccionar\" name=\"demo[]\" url=\"./upload.php\" accept=\"image/*\" :maxFileSize=\"1000000\" @upload=\"onUpload\" style=\"width: 650px\" />\n                <small v-if=\"errPhotoSign === true\" id=\"profilePic-help\" class=\"p-error\">Por favor, proporcione un nombre de usuario.</small>\n              </div>\n      