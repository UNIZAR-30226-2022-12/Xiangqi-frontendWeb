pplyDarkTheme();\n            else\n                this.applyLightTheme();\n        };\n        EventBus.on('change-theme', this.themeChangeListener);\n\n\t\tif (this.isDarkTheme()) {\n            this.applyDarkTheme();\n        }\n        else {\n            this.applyLightTheme();\n        }\n\t},\n\tbeforeUnmount() {\n        EventBus.off('change-theme', this.themeChangeListener );\n    },\n\tdata() {\n\t\treturn {\n\t\t\tlineData: {\n\t\t\t\tlabels: ['January', 'February', 'March', 'April', 'May', 'June', 'July'],\n\t\t\t\tdatasets: [\n\t\t\t\t\t{\n\t\t\t\t\t\tlabel: 'First Dataset',\n\t\t\t\t\t\tdata: [65, 59, 80, 81, 56, 55, 40],\n\t\t\t\t\t\tfill: false,\n\t\t\t\t\t\tbackgroundColor: '#2f4860',\n\t\t\t\t\t\tborderColor: '#2f4860',\n\t\t\t\t\t\ttension: .4\n\t\t\t\t\t},\n\t\t\t\t\t{\n\t\t\t\t\t\tlabel: 'Second Dataset',\n\t\t\t\t\t\tdata: [28, 48, 40, 19, 86, 27, 90],\n\t\t\t\t\t\tfill: false,\n\t\t\t\t\t\tbackgroundColor: '#00bb7e',\n\t\t\t\t\t\tborderColor: '#00bb7e',\n\t\t\t\t\t\ttension: .4\n\t\t\t\t\t}\n\t\t\t\t]\n\t\t\t},\n\t\t\tpieData: {\n\t\t\t\tlabels: ['A', 'B', 'C'],\n\t\t\t\tdatasets: [\n\t\t\t\t\t{\n\t\t\t\t\t\tdata: [300, 50, 100],\n\t\t\t\t\t\tbackgroundColor: [\n\t\t\t\t\t\t\t\"#FF6384\",\n\t\t\t\t\t\t\t\"#36A2EB\",\n\t\t\t\t\t\t\t\"#FFCE56\"\n\t\t\t\t\t\t],\n\t\t\t\t\t\thoverBackgroundColor: [\n\t\t\t\t\t\t\t\"#FF6384\",\n\t\t\t\t\t\t\t\"#36A2EB\",\n\t\t\t\t\t\t\t\"#FFCE56\"\n\t\t\t\t\t\t]\n\t\t\t\t\t}\n\t\t\t\t]\n\t\t\t},\n\t\t\tpolarData: {\n\t\t\t\tdatasets: [{\n\t\t\t\t\tdata: [\n\t\t\t\t\t\t11,\n\t\t\t\t\t\t16,\n\t\t\t\t\t\t7,\n\t\t\t\t\t\t3,\n\t\t\t\t\t\t14\n\t\t\t\t\t],\n\t\t\t\t\tbackgroundColor: [\n\t\t\t\t\t\t\"#FF6384\",\n\t\t\t\t\t\t\"#4BC0C0\",\n\t\t\t\t\t\t\"#FFCE56\",\n\t\t\t\t\t\t\"#E7E9ED\",\n\t\t\t\t\t\t\"#36A2EB\"\n\t\t\t\t\t],\n\t\t\t\t\tlabel: 'My dataset'\n\t\t\t\t}],\n\t\t\t\tlabels: [\n\t\t\t\t\t\"Red\",\n\t\t\t\t\t\"Green\",\n\t\t\t\t\t\"Yellow\",\n\t\t\t\t\t\"Grey\",\n\t\t\t\t\t\"Blue\"\n\t\t\t\t]\n\t\t\t},\n\t\t\tb