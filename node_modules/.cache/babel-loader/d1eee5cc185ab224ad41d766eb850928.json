\t<i class=\"pi pi-user\"></i>\n\t\t\t\t\t<span>Profile</span>\n\t\t\t\t</button>\n\t\t\t</li>\n\t\t\t<li>\n                <Button type=\"button\" icon=\"pi pi-search\" @click=\"toggle\" aria:haspopup=\"true\" aria-controls=\"overlay_panel\" />\n\n                <OverlayPanel ref=\"op\" appendTo=\"body\" :showCloseIcon=\"true\" id=\"overlay_panel\" style=\"width:450px\" :breakpoints=\"{'960px': '75vw'}\">\n\t\t\t\t\t<div id=\"layout-config\" :class=\"containerClass\">\n\t\t\t\t\t\t<div class=\"layout-config-content\">\n\t\t\t\t\t\t\t<h5 class=\"mt-0\">Component Scale</h5>\n\t\t\t\t\t\t\t<div class=\"config-scale\">\n\t\t\t\t\t\t\t\t<Button icon=\"pi pi-minus\" @click=\"decrementScale()\" class=\"p-button-text\" :disabled=\"scale === scales[0]\"/>\n\t\t\t\t\t\t\t\t<i class=\"pi pi-circle-on\" v-for=\"s of scales\" :class=\"{'scale-active': s === scale}\" :key=\"s\"/>\n\t\t\t\t\t\t\t\t<Button icon=\"pi pi-plus\"  @click=\"incrementScale()\" class=\"p-button-text\" :disabled=\"scale === scales[scales.length - 1]\" />\n\t\t\t\t\t\t\t</div>\n\t\t\t\t\t\t</div>\n\t\t\t\t\t</div>\n                </OverlayPanel>\n\t\t\t</li>\n\t\t</ul>\n\t</div>\n</template>\n\n<script>\nexport default {\n\tdata() {\n\t\treturn {\n\t\t\tlogged: false,\n\n\t\t\t//menu overlaypanel\n\t\t\tscale: 14,\n\t\t\tscales: [12,13,14,15,16]\n\t\t}\n\t},\n    methods: {\n        onMenuToggle(event) {\n            this.$emit('menu-toggle', event);\n        },\n\t\tonCog(event) {\n            this.$emit('config-toggle', event);\n        },\n\t\tonTopbarMenuToggle(event) {\n            this.$emit('topbar-menu-toggle', event);\n        },\n\t\ttopbarImage() {\n\t\t\treturn this.$appState.darkTheme ? 'images/logo-white.svg' : 'images/logo-dark.svg';\n\t\t},\n\t\tchangeTheme(event) {\n            this.$emit('change-theme', event);\n        },\n\t\t//Settings menu\n\t\ttoggle(event) {\n            this.$refs.op.toggle(event);\n        },\n\n\t\t//menu overlaypanel\n\t\tdecrementScale() {\n\t\t\tthis.scale--;\n\t\t\tthis.applyScale();\n\t\t},\n\t\tincrementScale() {\n\t\t\tthis.scale++;\n\t\t\tthis.applyScale();\n\t\t},\n\t\tapplyScale() {\n\t\t\tdocument.documentElement.style.fontSize = this.scale + 'px';\n\t\t},\n\t\t//------------------\t\n    },\n\tcomputed: {\n\t\tdarkTheme() {\n\t\t\treturn this.$appState.darkTheme;\n\t\t},\n\n\t\t//menu overlaypanel\n\t\tcontainerClass() {\n\t\t\treturn ['layout-config-active'];\n\t\t},\n\t\t//------------------\n\t}\n}\n</script>"]}]}