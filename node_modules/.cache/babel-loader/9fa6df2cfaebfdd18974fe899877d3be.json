,KAAjC,CAAuCrB,OAAvC,GAAiD,OAAjD;AACD,KArCI;AAsCLuB,cAtCK,wBAsCQ;AACX,UAAI,KAAKlB,QAAL,IAAiB,IAAjB,IAAyB,KAAKA,QAAL,IAAiB,EAA9C,EAAkD;AAChDmB,eAAO,CAACC,GAAR,CAAY,WAAZ;AACA,aAAKtB,YAAL,GAAoB,IAApB;AACF;;AACA,UAAI,KAAKM,QAAL,IAAiB,IAAjB,IAAyB,KAAKA,QAAL,IAAiB,EAA9C,EAAkD;AAChDe,eAAO,CAACC,GAAR,CAAY,WAAZ;AACA,aAAKrB,YAAL,GAAoB,KAApB;AACA,aAAKA,YAAL,GAAoB,IAApB;AACA,aAAKsB,YAAL,GAAoB,wCAApB;AACF;;AACA,UAAI,KAAKrB,QAAL,IAAiB,IAAjB,IAAyB,KAAKA,QAAL,IAAiB,EAA9C,EAAkD;AAChDmB,eAAO,CAACC,GAAR,CAAY,cAAZ;AACA,aAAKtB,YAAL,GAAoB,KAApB;AACF;;AACA,UAAI,KAAKM,QAAL,IAAiB,IAAjB,IAAyB,KAAKA,QAAL,IAAiB,EAA1C,IAAgD,KAAKA,QAAL,CAAcW,MAAd,IAAwB,CAA5E,EAA+E;AAC7EI,eAAO,CAACC,GAAR,CAAY,cAAZ,EAA4B,KAAKhB,QAAL,CAAcW,MAA1C;AACA,aAAKhB,YAAL,GAAoB,KAApB;AACF;;AACA,UAAI,KAAKK,QAAL,IAAiB,EAAjB,IAAuB,KAAKA,QAAL,CAAcW,MAAd,GAAuB,CAAlD,EAAoD;AAClDI,eAAO,CAACC,GAAR,CAAY,YAAZ,EAA0B,KAAKhB,QAAL,CAAcW,MAAxC;AACA,aAAKhB,YAAL,GAAoB,KAApB;AACA,aAAKA,YAAL,GAAoB,IAApB;AACA,aAAKsB,YAAL,GAAoB,gDAApB;AACF;AACD;AA/DI;AADI,CAAf","names":["methods","hazAlgo","display","isActive","isActiveMax","errUserLogin","errPassLogin","username","mail","name","birthday","password","confPassword","selectedCountry","texto","resize","tabName","selectedTab","i","x","document","getElementsByClassName","length","style","getElementById","checkLogin","console","log","textoPassLog"],"sourceRoot":"","sources":["/Users/Nacho/Desktop/webapp/src/Login.vue"],"sourcesContent":["<template>\n  <Button v-on:click=\"hazAlgo()\" label=\"Iniciar sesión / crear cuenta\" icon=\"pi pi-check\" iconPos=\"right\" />\n  <Dialog v-model:visible=\"display\" :header=\"texto\" :draggable=\"false\" :modal=\"true\" :class=\"{ 'altura': isActive, 'alturaMax': isActiveMax }\"> <!--v-model:visible asociado con variable contentStyle=\"padding: 0px;\"-->\n    <template #header>\n      <div class=\"card\">    \n        <Button class=\"p-button-outlined mr-3\" v-on:click=\"selectedTab('LogIn'), resize('LogIn')\" label=\"Iniciar sesión\" />\n        <Button class=\"p-button-outlined\" v-on:click=\"selectedTab('SignUp'), resize('SignUp')\" label=\"Crear una cuenta\" />\n      </div>\n    </template>\n    <div id=\"LogIn\" class=\"w3-container tabTypeJS\">\n      <div class=\"content-section implementation\">\n        <div class=\"card\">\n          <div class=\"p-fluid grid\" style=\"display: block !important;\">   \n            <!--FIELD-->\n            <div class=\"col-12\">\n              <h3> {{texto}}: </h3>\n            </div>\n            <!--USERNAME-->\n            <div class=\"field col-12\">\n              <label for=\"username\">Nombre de usuario</label>\n              <InputText id=\"username\" type=\"text\" v-model=\"username\" :class=\"{ 'p-invalid': errUserLogin}\"/>\n              <small v-if=\"errUserLogin === true\" style=\"transition: linear 0.5s; animation-duration: 0.5s;\" id=\"username-help\" class=\"p-error\">Por favor, proporcione un nombre de usuario.</small>\n            </div>\n            <!--PASSWORD-->\n            <div class=\"field col-12\">\n              <label for=\"password\">Contraseña</label>\n              <Password id=\"password\" v-model=\"password\" :class=\"{ 'p-invalid': errPassLogin}\" toggleMask></Password>\n              <small v-if=\"errPassLogin === true\" id=\"password-help\" class=\"p-error\">{{textoPassLog}}</small>\n            </div>\n          </div>\n        </div>\n      </div>  \n    </div>\n\n    <div id=\"SignUp\" class=\"w3-container tabTypeJS\" style=\"display:none\">\n      <div class=\"content-section implementation\">\n        <div class=\"card\">\n            <div class=\"p-fluid grid\" style=\"display: block !important;\">\n              <!--FIELD-->\n              <div class=\"col-12\">\n                <h3> {{texto}}: </h3>\n              </div>                          \n              <!--USERNAME-->\n              <div class=\"field col-12\">\n                <label for=\"username\">Nombre de usuario:</label>\n                <InputText id=\"username\" type=\"text\" v-model=\"username\" class=\"p-invalid\"/>\n                <small id=\"username-help\" class=\"p-error\">Por favor, proporcione un nombre de usuario.</small>\n              </div>\n              <!--MAIL-->\n              <div class=\"field col-12\">\n                <label for=\"mail\">E-mail:</label>\n                <InputText id=\"mail\" type=\"email\" v-model=\"mail\" class=\"p-invalid\"/>\n                <small id=\"mail-help\" class=\"p-error\">Por favor, proporcione un nombre de usuario.</small>\n              </div>\n              <!--NAME-->\n              <div class=\"field col-12\">\n                <label for=\"name\">Nombre y apellido:</label>\n                <InputText id=\"name\" type=\"text\" v-model=\"name\" class=\"p-invalid\"/>\n                <small id=\"name-help\" class=\"p-error\">Por favor, proporcione un nombre de usuario.</small>\n              </div>\n              <!--DATE OF BIRTH-->\n              <div class=\"field col-12\">\n                <label for=\"calendar\">Fecha de nacimiento:</label>\n                <Calendar id=\"calendar\" v-model=\"birthday\" class=\"p-invalid\"/>\n                <small id=\"calendar-help\" class=\"p-error\">Por favor, proporcione un nombre de usuario.</small>   \n              </div>\n              <!--COUNTRY-->\n              <div class=\"field col-12\">\n                <label for=\"country\">Seleccione su país de residencia:</label>\n                <Dropdown v-model=\"selectedCountry\" :options=\"countries\" optionLabel=\"name\" :filter=\"true\" placeholder=\"Select a Country\" :showClear=\"true\" class=\"p-invalid\">\n                  <template #value=\"slotProps\">\n                      <div class=\"country-item country-item-value\" v-if=\"slotProps.value\">\n                          <img src=\"src/assets/images/flag_placeholder.png\" :class=\"'flag flag-' + slotProps.value.code.toLowerCase()\" />\n                          <div>{{slotProps.value.name}}</div>\n                      </div>\n                      <span v-else>\n                          {{slotProps.placeholder}}\n                      </span>\n                  </template>\n                  <template #option=\"slotProps\">\n                      <div class=\"country-item\">\n                          <img src=\"src/assets/images/flag_placeholder.png\" :class=\"'flag flag-' + slotProps.option.code.toLowerCase()\" />\n                          <div>{{slotProps.option.name}}</div>\n                      </div>\n                  </template>\n                </Dropdown>\n                <small id=\"country-help\" class=\"p-error\">Por favor, proporcione un nombre de usuario.</small>\n              </div>\n              <!--FOTO DE PERFIL-->\n              <div class=\"field col-12\">\n                <label for=\"profilePic\">Foto de perfil:</label>\n                <FileUpload mode=\"basic\" chooseLabel=\"Seleccionar\" name=\"demo[]\" url=\"./upload.php\" accept=\"image/*\" :maxFileSize=\"1000000\" @upload=\"onUpload\" style=\"width: 650px\" />\n                <small id=\"profilePic-help\" class=\"p-error\">Por favor, proporcione un nombre de usuario.</small>\n              </div>\n              <!--PASSWORD-->\n              <div class=\"field col-12\">\n                <label for=\"password\">Contraseña:</label>\n                <Password id=\"password\" v-model=\"password\" class=\"p-invalid\" toggleMask></Password>\n                <small id=\"password-help\" class=\"p-error\">Por favor, proporcione un nombre de usuario.</small>\n              </div>\n              <!--CONF PASSWORD-->\n              <div class=\"field col-12\">\n                <label for=\"confPassword\">Confirmar contraseña:</label>\n                <Password id=\"confPassword\" v-model=\"confPassword\" class=\"p-invalid\" toggleMask></Password>\n                <small id=\"confPassword-help\" class=\"p-error\">Por favor, proporcione un nombre de usuario.</small>\n              </div>\n              <!--BUTTON--->\n              <!--<div class=\"field col-12 groupsepbtn\">\n                <Button label=\"Crear cuenta\" class=\"p-button-rounded\"/>\n              </div>-->\n            </div>\n        </div>\n      </div>\n    </div>\n    <template #footer>\n      <Button v-if=\"texto === 'Iniciar sesión'\" label=\"Iniciar sesión\" class=\"p-button-rounded\" v-on:click=\"checkLogin()\"/>\n      <Button v-else label=\"Crear cuenta\" class=\"p-button-rounded\"/>\n    </template>\n  </Dialog>\n</template>\n\n<script>\nexport default {\n  methods: {\n      hazAlgo() {\n        this.display = true;\n        this.isActive = true;\n        this.isActiveMax = false;\n        this.errUserLogin = false;\n        this.errPassLogin = false;\n        this.username = null;\n        this.mail = null;\n        this.name = null;\n        this.birthday = null;\n        this.password = null;\n        this.confPassword = null;\n        this.selectedCountry = null;\n        this.texto = \"Iniciar sesión\";\n      },\n      resize(tabName) {\n        //console.log(\"resize\");\n        if (tabName == \"SignUp\") {\n          this.isActive = false;\n          this.isActiveMax = true;\n          this.texto = \"Crear una cuenta\";\n        } else {\n          this.errUserLogin = false;\n          this.errPassLogin = false;\n          this.isActive = true;\n          this.isActiveMax = false;\n          this.texto = \"Iniciar sesión\";\n        }\n      },\n      selectedTab(tabName) {\n        var i;\n        var x = document.getElementsByClassName(\"tabTypeJS\");\n        for (i = 0; i < x.length; i++) {\n          x[i].style.display = \"none\";  \n        }\n        document.getElementById(tabName).style.display = \"block\";  \n      },\n      checkLogin() {\n        if (this.username == null || this.username == \"\") {\n          console.log(\"user null\");\n          this.errUserLogin = true;\n        }\n        if (this.password == null || this.password == \"\") {\n          console.log(\"pass null\");\n          this.errPassLogin = false;\n          this.errPassLogin = true;\n          this.textoPassLog = \"Por favor, proporcione una contraseña.\";\n        }\n        if (this.username != null && this.username != \"\") {\n          console.log(\"user NO null\");\n          this.errUserLogin = false;\n        }\n        if (this.password != null && this.password != \"\" && this.password.length >= 8) {\n          console.log(\"pass NO null\", this.password.length);\n          this.errPassLogin = false;\n        }\n        if (this.password != \"\" && this.password.length < 8){\n          console.log(\"pass corto\", this.password.length);\n          this.errPassLogin = false;\n          this.errPassLogin = true;\n          this.textoPassLog = \"La contraseña debe tener al menos 8 caracteres\";\n        }\n      },\n  }\n}\n</script>\n\n<style>\n#app {\n  font-family: Avenir, Helvetica, Arial, sans-serif;\n  -webkit-font-smoothing: antialiased;\n  -moz-osx-font-smoothing: grayscale;\n  text-align: center;\n}\n\nbody {\n  margin: 0;\n  height: 100%;\n  overflow-x: hidden;\n  overflow-y: auto;\n  background-color: var(--surface-a);\n  font-family: var(--font-family);\n  font-weight: 400;\n  color: var(--text-color);\n}\n\n\n/* DIALOG */\n.groupsepbtn {\n  padding-top: 30px !important;\n}\n\n.altura {\n  background: rgb(30,30,30);\n  width: 500px;\n  height: 460px;\n  animation-duration: 0.6s;\n  animation-name: lineInserted;\n  transition: height 0.6s, width 0.6s;\n}\n\n.alturaMax {\n  background: rgb(30,30,30);\n  width: 700px;\n  height: 961px;\n  animation-duration: 0.6s;\n  animation-name: lineInserted;\n  transition: height 0.6s, width 0.6s;\n}\n\n.p-dialog-footer {\n    padding-top: 1.5rem !important;\n    border-top: 1px solid !important;\n}\n\n.p-dialog-header {\n    padding-bottom: 1.5rem !important;\n    border-bottom: 1px solid !important;\n}\n\n</style>"]}]}